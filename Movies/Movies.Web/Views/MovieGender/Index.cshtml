@using Movies.Models;
@model IEnumerable<MovieGenderModel>
@{
    ViewData["Title"] = "Movie Gender";
}

<table>
    <tr>
        <th>Name</th>
        <th>Description</th>
        <th>Created At</th>
        <th>Modified At</th>
    </tr>
    @foreach(var movieGender in Model)
    {
       <tr>
           <td>@movieGender.Name</td>
           <td>@movieGender.Description</td>
           <td>@movieGender.CreatedAt.ToString("yyyy-MM-dd")</td>
           <td>@movieGender.ModifiedAt.ToString("yyyy-MM-dd")</td>
       </tr>
    }

    
    
</table>
